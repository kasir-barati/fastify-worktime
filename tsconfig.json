{
    "extends": "fastify-tsconfig",
    "compilerOptions": {
        "outDir": "./build",
        "target": "es2018",
        "module": "commonjs",
        "noImplicitAny": true,
        "strictNullChecks": true,
        "removeComments": true,
        "declaration": true,
        "sourceMap": true,
        "esModuleInterop": true
    },
    "exclude": ["node_modules", "build"]
}
